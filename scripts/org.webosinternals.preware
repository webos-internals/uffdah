#!/bin/sh

APPID=`basename $0`
APPNAME="Preware"
APPNOTE="Device Reboot required after restore"

source `dirname $0`/srf.app.info

source `dirname $0`/srf.app.databases

source `dirname $0`/srf.app.cookies

SRCDIR="/media/cryptofs/apps/etc/ipkg"
DSTSUB="ipkg"
FILES="*.conf *.conf.disabled"

source `dirname $0`/srf.app.files

exit 0
